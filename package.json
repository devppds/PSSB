{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:worker": "opennextjs-cloudflare build && npx esbuild .open-next/worker.js --bundle --outfile=.open-next/assets/_worker.js --format=esm --platform=node --target=esnext --external:__STATIC_CONTENT_MANIFEST --external:cloudflare:* --external:node:* --define:process.env.NODE_ENV='\"production\"'",
    "deploy": "wrangler deploy",
    "start": "next start",
    "lint": "eslint"
  },
  "dependencies": {
    "cloudinary": "^2.8.0",
    "next": "15.1.11",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@opennextjs/cloudflare": "^1.14.8",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.1.11",
    "typescript": "^5",
    "wrangler": "^4.54.0"
  }
}